{
    "configurations": [
        {
            "type": "command",
            "name": "Run trunk",
            "program": "trunk",
            "workingDir": "./",
            "args": [
                "serve"
            ],
        },
        {
            "type": "cargo",
            "name": "Debug Ppaass Agent UI",
            "cargoArgs": [
                "run",
                "--package",
                "ppaass-agent-ui-windows",
                "--bin",
                "ppaass-agent-ui-windows",
                "--no-default-features"
            ],
            "workingDir": "./",
        }
        ,
        {
            "type": "cargo",
            "name": "Run Ppaass Agent UI",
            "workingDir": "./",
            "cargoArgs": [
                "tauri",
                "dev"
            ],
        },
    ]
}