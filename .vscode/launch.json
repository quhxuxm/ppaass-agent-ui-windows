    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ppaass-agent-ui-windows'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ppaass-agent-ui-windows",
                    "--package=ppaass-agent-ui-windows"
                ],
                "filter": {
                    "name": "ppaass-agent-ui-windows",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'ppaass-agent-ui-windows'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=ppaass-agent-ui-windows",
                    "--package=ppaass-agent-ui-windows"
                ],
                "filter": {
                    "name": "ppaass-agent-ui-windows",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ppaass-agent-ui-windows-ui'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ppaass-agent-ui-windows-ui",
                    "--package=ppaass-agent-ui-windows-ui"
                ],
                "filter": {
                    "name": "ppaass-agent-ui-windows-ui",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'ppaass-agent-ui-windows-ui'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=ppaass-agent-ui-windows-ui",
                    "--package=ppaass-agent-ui-windows-ui"
                ],
                "filter": {
                    "name": "ppaass-agent-ui-windows-ui",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "Start trunk server",
            "script": "trunk serve"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Tauri Development Debug",
            "cargo": {
                "args": [
                    "build",
                    "--manifest-path=./src-tauri/Cargo.toml",
                    "--no-default-features"
                ]
            },
            // task for the `beforeDevCommand` if used, must be configured in `.vscode/tasks.json`
            // "preLaunchTask": "tauri-before-dev"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Tauri Production Debug",
            "cargo": {
                "args": [
                    "build",
                    "--release",
                    "--manifest-path=./src-tauri/Cargo.toml"
                ]
            },
            // task for the `beforeBuildCommand` if used, must be configured in `.vscode/tasks.json`
            // "preLaunchTask": "tauri-before-build"
        }
    ]
}